// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "alt+x",
        "command": "workbench.action.showCommands"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.navigateLeft"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.navigateRight"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.navigateUp"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.navigateDown"
    },
    {
        "key": "cmd+;",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+;",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "!terminalFocus"
    },
    {
        "key": "backspace",
        "command": "language-julia.plotpane-delete",
        "when": "jlplotpaneFocus"
    },
    {
        "key": "shift+backspace",
        "command": "language-julia.plotpane-delete-all",
        "when": "jlplotpaneFocus"
    },
    // buggy when doesn't work { "key": "q", "command": "workbench.action.closeActiveEditor", "when": "jlplotpaneFocus" },
    // { "before":["<leader>", "m", "e" ], "commands": ["language-julia.executeJuliaBlockInREPL"]}, 
    // { "before":[",", "l" ], "commands": ["language-julia.executeJuliaCodeInREPL"]}, 
    {
        "key": ", r",
        "command": "editor.action.rename",
        "when": "editorTextFocus && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": ", s",
        "command": "language-julia.selectBlock",
        "when": "editorTextFocus && editorLangId == 'julia' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "escape",
        "command": "multiCommand.juliaEscape",
        "when": "editorTextFocus && editorLangId == 'julia' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": ", e",
        "command": "language-julia.executeCodeBlockOrSelectionAndMove",
        "when": "editorTextFocus && editorLangId == 'julia' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": ", shift+e",
        "command": "language-julia.executeCodeBlockOrSelection",
        "when": "editorTextFocus && editorLangId == 'julia' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": ", l",
        "command": "language-julia.executeJuliaCodeInREPL",
        "when": "editorTextFocus && editorLangId == 'julia' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": ", c",
        "command": "language-julia.executeCellAndMove",
        "when": "editorTextFocus && editorLangId == 'julia' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": ", shift+c",
        "command": "language-julia.executeCell",
        "when": "editorTextFocus && editorLangId == 'julia' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": ", b",
        "command": "language-julia.executeFile",
        "when": "editorTextFocus && editorLangId == 'julia' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": ", h",
        "command": "editor.action.showHover",
        "when": "editorTextFocus && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "ctrl+a",
        "command": "cursorLineStart",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+e",
        "command": "cursorLineEnd",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+'",
        "command": "workbench.action.toggleMaximizedPanel"
    },
    {
        "key": "cmd+o",
        "command": "-workbench.action.files.openFileFolder"
    },
    {
        "key": "cmd+o",
        "command": "-workbench.action.files.openLocalFileFolder",
        "when": "remoteFileDialogVisible"
    },
    {
        "key": "cmd+o",
        "command": "workbench.action.quickSwitchWindow"
    },
    {
        "key": "ctrl+=",
        "command": "workbench.action.navigateForward"
    },
    {
        "key": "alt+/",
        "command": "-fsi.SendLine",
        "when": "editorFocus && !findWidgetVisible && editorLangId == 'fsharp'"
    },
    {
        "key": "alt+/",
        "command": "-gitlens.gitCommands",
        "when": "!gitlens:disabled && config.gitlens.keymap == 'alternate'"
    },
    {
        "key": "alt+/",
        "command": "insertBestCompletion"
    },
    {
        "key": "tab",
        "command": "-insertBestCompletion",
        "when": "atEndOfWord && textInputFocus && !hasOtherSuggestions && !inSnippetMode && !suggestWidgetVisible && config.editor.tabCompletion == 'on'"
    },
    // {
    //     "key": "[ b",
    //     "command": "workbench.action.previousEditor",
    //     "when": "!inputFocus"
    // },
    // {
    //     "key": "] b",
    //     "command": "workbench.action.nextEditor",
    //     "when": "!inputFocus || (vim.active && vim.mode != 'Insert')"
    // },
    // {
    //     "key": "[ j",
    //     "command": "workbench.action.navigateBack",
    //     "when": "!inputFocus || (vim.active && vim.mode != 'Insert')"
    // },
    // {
    //     "key": "] j",
    //     "command": "workbench.action.navigateForward",
    //     "when": "!inputFocus || (vim.active && vim.mode != 'Insert')"
    // },
]