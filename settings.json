{
    "workbench.dialogs.customEnabled": true,
    // Customizes which terminal applicaotion to run on OS X.
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
    // "vim.insertModeKeyBindings": [
    //     {
    //         "before": ["i", "i"],
    //         "after": ["<Esc>"]
    //    }
    // ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "w",
                "m"
            ],
            "after": [],
            "commands": [
                //  {
                //     "command": "workbench.action.problems.focus",
                //     "args": []
                // }
                {
                    "command": "workbench.action.maximizeEditor",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "s"
            ],
            "commands": [
                "workbench.action.files.save"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "s"
            ],
            "commands": [
                "workbench.action.openSettings"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "t"
            ],
            "commands": [
                "workbench.action.terminal.toggleTerminal"
            ]
        },
        {
            "before": [
                "<leader>",
                "q"
            ],
            "after": [
                "<leader>",
                "<leader>",
                "b"
            ]
        },
        {
            "before": [
                "<leader>",
                "e"
            ],
            "after": [
                "<leader>",
                "<leader>",
                "w"
            ]
        },
        {
            "before": [
                "<leader>",
                "j"
            ],
            "after": [
                "<leader>",
                "<leader>",
                "<leader>",
                "j"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "K"
            ],
            "commands": [
                "workbench.action.openGlobalKeybindingsFile"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "d"
            ],
            "commands": [
                "workbench.action.openDefaultKeybindingsFile"
            ]
        },
        // { "before":["[", "j" ], "commands": ["workbench.action.navigateBack"]}, 
        // { "before":["]", "j" ], "commands": ["workbench.action.navigateForward"]}, 
        // { "before":["[", "b" ], "commands": ["workbench.action.previousEditor"]}, 
        // { "before":["]", "b" ], "commands": ["workbench.action.nextEditor"]},  
        {
            "before": [
                "<leader>",
                "h"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        } /* also: , h */,
        {
            "before": [
                "<leader>",
                "s",
                "k"
            ],
            "after": [],
            "commands": [
                {
                    // "command": "workbench.action.openDefaultKeybindingsFile",
                    "command": "workbench.action.openGlobalKeybindings",
                    "args": []
                }
            ]
        },
        {
            "before": [
                ";"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.showCommands",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "U"
            ],
            "commands": [
                "redo"
            ]
        }
    ],
